  "license": "MIT",
  "type": "module",
  "bin": {
    "namefix": "bin/namefix.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=20 <23",
    "pnpm": ">=9"
  },
  "volta": {
    "node": "22.6.0",
    "pnpm": "9.0.0"
  },
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "start": "node bin/namefix.js",
    "biome": "biome check --apply . || true",
    "format": "biome format --write . || true",
    "lint": "biome lint . || true",
    "prepare": "command -v husky >/dev/null 2>&1 && husky install || true",
    "test": "vitest run || echo 'tests pending'"
  },
  "keywords": [
    "macos",
